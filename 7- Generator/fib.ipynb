   "source": [
    "def fib(max_num):\n",
    "    a = 0\n",
    "    b = 1\n",
    "\n",
    "    for i in range(max_num):\n",
    "        yield a\n",
    "        temp = a\n",
    "        a = b\n",
    "        b = temp + b\n",
    "\n",
    "for num in fib(20):\n",
    "    print(num)        \n"
   ]
